{
    "model_type": "EmbeddingNoiseClassifier",
    "input_channels": 1,
    "output_channels": 1,
    "embed_dim": 256,
    "hidden_dims": [512, 256, 128],
    "dropout": 0.1,
    "activation": "relu",
    "norm": true,
    "sigmoid": false,
    
    "training": {
        "epochs": 100,
        "batch_size": 128,
        "learning_rate": 1e-4,
        "weight_decay": 1e-5,
        "scheduler": {
            "type": "StepLR",
            "step_size": 50,
            "gamma": 0.5
        }
    },
    
    "data": {
        "clean_embeddings_path": "/projects/0/prjs0951/Varun/data/embeddings/clean_embeddings.pt",
        "corrupted_embeddings_path": "/projects/0/prjs0951/Varun/data/embeddings/corrupted_embeddings.pt",
        "noise_levels_path": "/projects/0/prjs0951/Varun/data/embeddings/noise_levels.pt",
        "train_split": 0.8,
        "val_split": 0.2
    }
} 